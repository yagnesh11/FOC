#include<stdio.h>
int main()
{
	int num,rem,sum=0,result;
	printf("Enter the number :");
	scanf("%d",&num);
	num = result;
	
	while(num != 0)
	{
		rem = num%10;
		sum = sum + (rem * rem * rem);
		num = num/10;
	}
	if(sum  = num)
	{
		printf("%d is armstrong number",num);
	}
	else
	{
		printf("%d is not armstrong number",num);
	}
}
